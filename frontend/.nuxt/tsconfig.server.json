{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "allowJs": true,
    "resolveJsonModule": true,
    "jsx": "preserve",
    "jsxFactory": "h",
    "jsxFragmentFactory": "Fragment",
    "paths": {
      "#imports": [
        "/root/.nuxt/types/nitro-imports"
      ],
      "#paths": [
        "/root/node_modules/nuxt/dist/core/runtime/nitro/paths"
      ],
      "~": [
        "/root"
      ],
      "~/*": [
        "/root/*"
      ],
      "@": [
        "/root"
      ],
      "@/*": [
        "/root/*"
      ],
      "~~": [
        "/root"
      ],
      "~~/*": [
        "/root/*"
      ],
      "@@": [
        "/root"
      ],
      "@@/*": [
        "/root/*"
      ],
      "assets": [
        "/root/assets"
      ],
      "assets/*": [
        "/root/assets/*"
      ],
      "public": [
        "/root/public"
      ],
      "public/*": [
        "/root/public/*"
      ],
      "pinia": [
        "/root/node_modules/pinia/dist/pinia"
      ]
    }
  },
  "include": [
    "/root/.nuxt/types/nitro-nuxt.d.ts",
    "./types/nitro.d.ts",
    "../**/*",
    "../server/**/*"
  ]
}